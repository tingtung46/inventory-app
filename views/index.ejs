<html>
  <head>
    <title>Products Inventory</title>
  </head>

  <body>
    <%- include('partials/header.ejs') %>

    <main>
      <%- include('partials/sidebar.ejs', { brands, categories }) %>

      <section>
        <% if (!items.length) { %>
        <p>Item not found.</p>
        <% } else if (locals.items) { %> <%- include('partials/cards.ejs',
        {items: items}) %> <% } %>

        <a href="/new">Add new item</a>
      </section>
    </main>
  </body>
</html>
